<launch>
  <env name="ROS_MASTER_URI" value="http://localhost:11315"/>
  <include file="$(find rocon_app_manager)/launch/concert_client.launch">
    <arg name="robot_name" value="announcer"/>
    <arg name="robot_type" value="pc"/>
    <arg name="concert_name" value="turtle_concert"/>
    <arg name="app_lists" value="$(find turtle_race_concert)/apps/turtle_race.rapps"/>
    <arg name="gateway_watch_loop_period" value="2"/> <!-- flip faster, convenient -->
  </include>
</launch>
