<!-- 
  Launches the gateway hub (redis data store) without publishing
  it via zeroconf (avahi/bonjour/jmdns).
-->
<launch>
  <node pkg="rocon_hub" type="hub.py" name="gateway_hub">
    <rosparam command="load" file="$(find rocon_hub)/param/default.yaml" />
    <rosparam param="name">Buccaneer Hub</rosparam>
  </node>
</launch>
